{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["src/"],
  "out": "docs",
  "includeVersion": false,
  "exclude": ["**/*.test.ts"],
  "plugin": ["@8hobbies/typedoc-plugin-404", "typedoc-plugin-rename-defaults", "typedoc-material-theme"],
  "entryPointStrategy": "expand",
  "hideGenerator": true,
  "cleanOutputDir": true,
  "page404Content": "This poage does not exist",
  "useHostedBaseUrlForAbsoluteLinks": true,
  "hostedBaseUrl": "http://127.0.0.1:5500",
  "lang": "en",
  "themeColor": "#2f3236"
}
